{
  "name": "h5-imageviewer",
  "version": "0.6.8",
  "description": "h5 imageviewer for mobile platform, support all web frameworks",
  "main": "dist/bundle.js",
  "files": [
    "dist",
    "src/index.d.ts"
  ],
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.pro.js",
    "preview": "webpack --config webpack.preview.js",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server --config webpack.dev.js --disableHostCheck=true",
    "appdev": "webpack-dev-server --config webpack.dev.app.js --disableHostCheck=true",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "standard"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|png)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  },
  "dependencies": {
    "alloyfinger": "^0.1.16"
  },
  "browserslist": [
    "defaults",
    "Chrome >= 49",
    "Firefox >= 48",
    "Safari >= 9",
    "ChromeAndroid >= 38",
    "ios_saf >= 9",
    "Android >= 3",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/react": "^9.3.3",
    "autoprefixer": "^9.6.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.9",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/src/example/"
    ],
    "globals": [
      "Image"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TUBB/h5-imageviewer.git"
  },
  "keywords": [
    "h5",
    "mobile",
    "image",
    "viewer"
  ],
  "author": "TUBB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TUBB/h5-imageviewer/issues"
  },
  "homepage": "https://github.com/TUBB/h5-imageviewer#readme"
}
